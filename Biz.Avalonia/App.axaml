<Application
    x:Class="Biz.Avalonia.App"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:avalonia="clr-namespace:Biz.Avalonia"
    xmlns:local="using:Biz.Avalonia"
    xmlns:theme="using:Biz.Theme"
    RequestedThemeVariant="Default">

    <Application.DataTemplates>
        <avalonia:ViewLocator />
    </Application.DataTemplates>

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.ThemeDictionaries />
        </ResourceDictionary>
    </Application.Resources>

    <Application.Styles>
        <theme:BizTheme DensityStyle="Compact" />
    </Application.Styles>
</Application>
