<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0-ios</TargetFramework>
    <SupportedOSPlatformVersion>13.0</SupportedOSPlatformVersion>
    <Nullable>enable</Nullable>
    <AssemblyName>Biz.Shell.iOS</AssemblyName>
    <RootNamespace>Biz.Shell.iOS</RootNamespace>
    <PackageId>Biz.Shell.iOS</PackageId>
    <Authors>Chris Bordeman</Authors>
    <Company>Chris Bordeman</Company>
    <LangVersion>default</LangVersion>
    <Product />
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DefineConstants>__IOS__;__MOBILE__;__UNIFIED__;TRACE;iOS</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DefineConstants>__IOS__;__MOBILE__;__UNIFIED__;TRACE;iOS</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Avalonia.iOS" Version="11.3.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Biz.Platform.Mobile\Biz.Platform.Mobile.csproj" />
    <ProjectReference Include="..\Biz.Shell\Biz.Shell.csproj" />
  </ItemGroup>
</Project>
